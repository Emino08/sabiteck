{
    "name": "sabiteck/backend",
    "description": "Sabiteck Backend API",
    "type": "project",
    "require": {
        "php": "^8.1",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.4",
        "firebase/php-jwt": "^6.3",
        "geoip2/geoip2": "^2.13",
        "mobiledetect/mobiledetectlib": "^2.8",
        "dompdf/dompdf": "^1.2",
        "ratchet/pawl": "^0.3",
        "react/socket": "^1.11",
        "phpoffice/phpspreadsheet": "^1.28",
        "matomo/device-detector": "^5.0",
        "whichbrowser/parser": "^2.0",
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sabiteck\\": "src/",
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "test": "phpunit"
    }
}
